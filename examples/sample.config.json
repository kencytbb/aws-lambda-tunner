{
  "function_arn": "arn:aws:lambda:us-east-1:123456789012:function:my-api-handler",
  "payload": {
    "httpMethod": "GET",
    "path": "/users",
    "queryStringParameters": {
      "limit": "10"
    }
  },
  "memory_sizes": [256, 512, 1024, 1536, 2048],
  "iterations": 15,
  "strategy": "balanced",
  "concurrent_executions": 5,
  "timeout": 300,
  "warmup_runs": 2,
  "output_dir": "./tuning-results"
}